{
	"$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
	"organizeImports": {
	  "enabled": true
	},
	"formatter": {
	  "indentStyle": "space",
	  "indentWidth": 2,
	  "lineEnding": "lf",
	  "ignore": [],
	  "formatWithErrors": false,
	  "lineWidth": 80
	},
	"javascript": {
	  "formatter": {
		"quoteStyle": "single",
		"trailingCommas": "es5",
		"semicolons": "asNeeded"
	  }
	},
	"json": {
	  "formatter": {
		"trailingCommas": "none"
	  }
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true,
		"a11y": {
		  "noBlankTarget": "off",
		  "useButtonType": "off"

		},
		"correctness": {
		  "noUnusedImports": "error",
		  "noConstAssign": "error",
		  "noUnusedVariables": "error"
		},
		"style": {
		  "noNonNullAssertion": "off"
		}
	  }
	}
  }
  